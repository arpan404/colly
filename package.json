{
  "name": "colly-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend/web-veil-maker && npm run dev",
    "install:all": "npm install && cd backend && npm install && cd ../frontend/web-veil-maker && npm install"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
