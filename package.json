{
  "name": "colly",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "migrate:test": "export DATABASE_URL=\"$DATABASE_URL_TEST\" && npx drizzle-kit migrate",
    "test:docker": "docker-compose up -d && echo 'Waiting for Postgres...' && for i in {1..30}; do docker-compose exec -T postgres pg_isready -U postgres -d colly_test && break || sleep 1; done && export DATABASE_URL_TEST=\"postgresql://postgres:password@localhost:5432/colly_test\" && export DATABASE_URL=\"$DATABASE_URL_TEST\" && npx drizzle-kit migrate && vitest run",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@trpc/client": "^11.7.1",
    "@trpc/next": "^11.7.1",
    "@trpc/server": "^11.7.1",
    "@types/pg": "^8.15.6",
    "@types/redis": "^4.0.10",
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "jsonwebtoken": "^9.0.2",
    "next": "16.0.3",
    "postgres": "^3.4.7",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "redis": "^5.10.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "@vitest/ui": "^4.0.10",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "jsdom": "^27.2.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.10"
  }
}
